{
    "name": "rudra/model",
    "description": "Rudra framework",
    "type": "library",
    "keywords": [
        "rudra",
        "model"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jagepard",
            "email": "jagepard@yandex.ru",
            "homepage": "https://jagepard.ru",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "jagepard@yandex.ru",
        "issues": "https://github.com/Jagepard/Rudra-Model/issues",    
        "source": "https://github.com/Jagepard/Rudra-Model"    
    },
    "require": {
        "php": ">=8.3",
        "rudra/container" : "self.version",
        "rudra/redirect"  : "self.version",
        "rudra/exception" : "self.version",
        "rudra/pagination": "self.version",
        "rudra/validation": "self.version",
        "rudra/docs"      : "self.version",
        "ext-pdo"         : "*"
    },
    "autoload"         : {
        "psr-4": {
            "Rudra\\Model\\": "src/"
        }
    },
    "autoload-dev"     : {
        "psr-4": {
            "Rudra\\Model\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "platform-check": false
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
